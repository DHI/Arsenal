    "svg-url-loader": "~6.0.0",
    "ts-loader": "~7.0.5",
    "ts-node": "~8.10.2",
    "typescript": "~3.9.5",
    "url-loader": "~4.1.0",
    "webpack": "~4.43.0",
    "webpack-bundle-analyzer": "~3.8.0",
