{
  "scripts": {
    "dev": "start-storybook --quiet --ci -p 9002 -c .storybook",
    "lint": "concurrently 'yarn lint:eslint' 'yarn lint:ts'",
    "lint:eslint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "lint:ts": "tsc --noEmit --incremental false --tsBuildInfoFile null",
    "start": "webpack-dev-server --host 0.0.0.0 --port 9001 --hide-modules",
    "build": "yarn build:clean && yarn build:prodEnvFile; webpack -p --env.production --color --hide-modules --display-optimization-bailout",
    "build:prodEnvFile": "shx test -e ./.env.production || shx cp .env.example .env.production",
    "build:clean": "rimraf ./build/*",
    "build:depGraph": "depcruise --max-depth 1 --output-type dot client | dot -T svg > dependencygraph.svg",
    "bundleAnalyzer": "yarn -s webpack -p --profile --json > build/stats.json && yarn -s webpack-bundle-analyzer ./build/stats.json ./build"
  },
  "dependencies": {
    "@material-ui/core": "~4.10.1",
    "@material-ui/icons": "~4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@types/classnames": "^2.2.10",
    "@types/debug": "^4.1.5",
    "@types/react-helmet": "^6.0.0",
    "bluebird": "~3.7.2",
    "classnames": "^2.2.6",
    "debug": "^4.1.1",
    "history": "5",
    "mobx": "~5.15.4",
    "mobx-react-lite": "~2.0.7",
    "mobx-state-tree": "~3.16.0",
    "mobx-utils": "^5.6.1",
    "mst-log": "^1.0.0",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-helmet": "^6.1.0",
    "styled-components": "~5.1.1",
    "tiny-mst-router": "~2.0.0",
    "typeface-roboto": "~0.0.75"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@types/bluebird": "~3.5.32",
    "@types/dotenv-webpack": "~1.8.0",
    "@types/html-webpack-plugin": "~3.2.3",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/styled-components": "~5.1.0",
    "@types/webpack": "~4.41.17",
    "@types/webpack-bundle-analyzer": "~3.8.0",
    "@types/webpack-dev-server": "~3.11.0",
    "@types/webpack-env": "~1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "css-loader": "~3.6.0",
    "dependency-cruiser": "^9.8.0",
    "dot": "^1.1.3",
    "dotenv-cli": "^3.1.0",
    "dotenv-webpack": "~1.8.0",
    "eslint": "~7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard-typescript-prettier": "~2.2.3",
    "eslint-import-resolver-webpack": "~0.12.1",
    "eslint-plugin-babel": "~5.3.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jsx-a11y": "~6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "~1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "~7.20.0",
