{
  "scripts": {
    "dev": "start-storybook --quiet --ci -p 9002 -c .storybook",
    "lint": "concurrently 'yarn lint:eslint' 'yarn lint:ts'",
    "lint:eslint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "lint:ts": "tsc --noEmit",
    "build": "yarn build:clean; NODE_ENV=production webpack -p --env.production --color --hide-modules --display-optimization-bailout",
    "build:storybook": "build-storybook -c .storybook -o .storybook/build",
    "build:clean": "rimraf ./build/*",
    "preversion": "yarn lint && yarn test && yarn build",
    "makePublish": "cd build && npm publish",
    "build:depGraph": "depcruise --include-only '^components' --output-type dot components | dot -T svg > dependencygraph.svg",
    "bundleAnalyzer": "yarn -s webpack -p --profile --json > build/stats.json && yarn -s webpack-bundle-analyzer ./build/stats.json ./build"
  },
  "dependencies": {
    "@material-ui/core": "~4.9.14",
    "@material-ui/icons": "~4.9.1",
    "@types/debug": "^4.1.5",
    "@types/lodash": "4.14.108",
    "case": "^1.6.3",
    "debug": "^4.1.1",
    "lodash": "4.17.19",
    "mobx": "~5.15.4",
    "mobx-react-lite": "~1.5.2",
    "mobx-state-tree": "~3.16.0",
    "mobx-utils": "^5.5.7",
    "react": "~16.12.0",
    "react-dom": "~16.12.0",
    "react-router": "~5.1.2",
    "react-router-dom": "~5.1.2",
    "react-swipeable-views": "^0.13.9",
    "react-virtualized": "^9.21.2",
    "styled-components": "~5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/copy-webpack-plugin": "5",
    "@types/dotenv-webpack": "~1.7.0",
    "@types/react": "~16.9.23",
    "@types/react-dom": "~16.9.5",
    "@types/react-router-dom": "~5.1.3",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/react-virtualized": "^9.21.8",
    "@types/styled-components": "~4.4.2",
    "@types/webpack": "~4.41.5",
    "@types/webpack-bundle-analyzer": "~2.13.3",
    "@types/webpack-env": "~1.15.1",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "5",
    "css-loader": "~3.4.2",
    "dependency-cruiser": "^9.8.0",
    "dotenv-cli": "^3.1.0",
    "dotenv-webpack": "~1.7.0",
    "eslint-config-standard-typescript-prettier": "~2.2.2",
    "fork-ts-checker-webpack-plugin": "~4.1.3",
    "rimraf": "~3.0.0",
    "style-loader": "~1.1.3",
    "ts-loader": "~7.0.1",
    "ts-node": "~8.9.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "~3.9.3",
    "url-loader": "~3.0.0",
    "webpack": "~4.42.1",
    "webpack-bundle-analyzer": "~3.7.0",
    "webpack-cli": "~3.3.10"
  },
  "prettier": "eslint-config-standard-typescript-prettier/prettier",
  "name": "@dhi/webdev-arsenal",
  "version": "0.1.1"
}
