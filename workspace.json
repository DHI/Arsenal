{
  "version": 2,
  "projects": {
    "@dhi/arsenal.jsonform": {
      "root": "packages/jsonform",
      "sourceRoot": "packages/jsonform/src",
      "projectType": "library"
    },
    "@dhi/arsenal.models": {
      "root": "packages/models",
      "sourceRoot": "packages/models/src",
      "projectType": "library"
    },
    "@dhi/arsenal.ui": {
      "root": "packages/ui",
      "sourceRoot": "packages/ui/src",
      "projectType": "library"
    },
    "react-app": {
      "root": "apps/react-app"
    }
    
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "react-app",
  "cli": {
    "analytics": false,
    "defaultCollection": "@nrwl/react",
    "pacakgeManager": "pnpm"
  }
}
